{
  "configuration": {
    "appenders": {
      "Console": {
        "name": "JsonConsole",
        "target": "SYSTEM_OUT",
        "JsonLayout": {
          "compact": true,
          "eventEol": true,
          "properties": true
        }
      }
    },
    "loggers": {
      "logger": [
        {"name": "io.helidon", "level": "info"}
      ],
      "root": {
        "level": "info",
        "appenderRef": {
          "ref": "JsonConsole"
        }
      }
    }
  }
}
